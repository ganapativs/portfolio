{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^0.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "@rehooks/window-size": "^1.0.2",
    "animate.css": "^3.7.0",
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "16.7.0-alpha.2",
    "react-cache": "2.0.0-alpha.1",
    "react-dom": "16.7.0-alpha.2",
    "react-scripts": "^2.1.3",
    "styled-components": "^4.1.3",
    "webfontloader": "^1.6.28"
  }
}
